{
  "name": "Vendor Portal Listener",
  "description": "Collects network data on vendor portal pages.",
  "version": "0.1",
  "permissions": [
    "debugger",
    "tabs",
    "storage",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["network-listener.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Vendor Portal Listener"
  },
  "manifest_version": 2,
  "host_permissions": [
    "http://*/",
    "https://*/"
  ]
}